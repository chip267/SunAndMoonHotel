<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFF0CB">
        <RelativeLayout
            app:layout_constraintTop_toTopOf="parent"

            android:layout_width="match_parent"
            android:layout_height="287dp"
            android:background="@drawable/bg"

            tools:ignore="MissingConstraints" >

        </RelativeLayout>

        <ImageView
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="48dp"
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/menu_button"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="73dp"
            android:layout_height="41dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/logo"
            android:layout_marginTop="54dp"
            android:layout_marginLeft="267dp"
            tools:ignore="MissingConstraints" />

        <RelativeLayout
            android:id="@+id/plastic"
            android:layout_width="341dp"
            android:layout_height="236dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:background="@drawable/plastic_bg"

            android:layout_marginTop="108dp"
            tools:ignore="MissingConstraints"
            />

        <RelativeLayout
            android:id="@+id/boxBookandForecast"
            android:layout_width="232dp"
            android:layout_height="90dp"
            app:layout_constraintTop_toTopOf="@+id/plastic"
            app:layout_constraintLeft_toLeftOf="@+id/plastic"
            android:layout_marginTop="122dp"
            android:layout_marginLeft="55dp"
            android:background="@drawable/booking_number_bg"
            tools:ignore="MissingConstraints" />

        <RelativeLayout
            android:id="@+id/Line"
            android:layout_width="1dp"
            android:layout_height="90dp"
            app:layout_constraintTop_toTopOf="@+id/boxBookandForecast"
            app:layout_constraintLeft_toLeftOf="@+id/boxBookandForecast"
            android:layout_marginTop="0.01dp"
            android:layout_marginLeft="116dp"
            android:background="@drawable/btnlogin"
            tools:ignore="MissingConstraints" />
        <TextView
            android:id="@+id/tvTextBooking"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="@+id/boxBookandForecast"
            app:layout_constraintTop_toTopOf="@+id/boxBookandForecast"
            android:layout_marginTop="60dp"
            android:layout_marginLeft="22dp"
            android:text="CHECKIN"
            android:textSize="15dp"
            android:gravity="top"
            android:fontFamily="@font/librefranklinmedium"
            android:textColor="#FFF0CB"
            />

        <TextView
            android:id="@+id/tvTextAppearBooking"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="@+id/boxBookandForecast"
            app:layout_constraintTop_toTopOf="@+id/boxBookandForecast"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="45dp"
            android:text="8"
            android:textSize="40dp"
            android:gravity="top"
            android:fontFamily="@font/librefranklinmedium"
            android:textColor="#F2AE1C"
            />
        <TextView
            android:id="@+id/tvTextAppearForecast"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="@+id/boxBookandForecast"
            app:layout_constraintTop_toTopOf="@+id/boxBookandForecast"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="159dp"
            android:text="5"
            android:textSize="40dp"
            android:gravity="top"
            android:fontFamily="@font/librefranklinmedium"
            android:textColor="#F2AE1C"
            />

        <TextView
            android:id="@+id/tvTextForecast"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="@+id/boxBookandForecast"
            app:layout_constraintTop_toTopOf="@+id/boxBookandForecast"
            android:layout_marginTop="60dp"
            android:layout_marginLeft="132dp"
            android:text="CHECKOUT"
            android:textSize="15dp"
            android:gravity="top"
            android:fontFamily="@font/librefranklinmedium"
            android:textColor="#FFF0CB"
            />


        <TextView
            android:id="@+id/tv1"
            android:layout_width="101dp"
            android:layout_height="30dp"
            app:layout_constraintLeft_toLeftOf="@+id/plastic"
            app:layout_constraintTop_toTopOf="@+id/plastic"
            app:layout_constraintRight_toRightOf="@+id/plastic"
            android:layout_marginTop="26dp"
            android:layout_marginRight="201dp"
            android:text="Today"
            android:textSize="35dp"
            android:lineSpacingExtra="-12sp"
            android:gravity="top"
            android:fontFamily="@font/canelamediumtrial"
            android:textColor="#430000"
            />

        <TextView
            android:id="@+id/tv2"
            android:layout_width="88dp"
            android:layout_height="30dp"
            app:layout_constraintLeft_toLeftOf="@+id/plastic"
            app:layout_constraintTop_toTopOf="@+id/plastic"
            app:layout_constraintRight_toRightOf="@+id/plastic"
            android:layout_marginTop="69dp"
            android:layout_marginRight="212dp"
            android:text="11:03 AM"
            android:textSize="20dp"
            android:fontFamily="@font/canelamediumtrial"
            android:textColor="#430000"
            tools:ignore="DuplicateIds" />

        <TextView
            android:id="@+id/tv3"
            android:layout_width="129dp"
            android:layout_height="30dp"
            app:layout_constraintLeft_toLeftOf="@+id/tv2"
            app:layout_constraintTop_toTopOf="@+id/plastic"
            app:layout_constraintRight_toRightOf="@+id/plastic"
            android:layout_marginTop="69dp"
            android:layout_marginLeft="43dp"
            android:text="26th October"
            android:textSize="20dp"
            android:fontFamily="@font/canelamediumtrial"
            android:textColor="#430000"
            tools:ignore="DuplicateIds" />

        <RelativeLayout
            android:id="@+id/Slider"
            android:layout_width="306dp"
            android:layout_height="38dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="373dp"

            android:background="@drawable/btn_allrooms"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/tvHousekeeping"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="@+id/Slider"
            app:layout_constraintTop_toTopOf="@+id/Slider"
            app:layout_constraintBottom_toBottomOf="@+id/Slider"
            android:layout_marginLeft="50dp"
            android:text="Manage"
            android:textSize="15dp"
            android:fontFamily="@font/librefranklinmedium"
            android:textColor="#FFFFFF"
            />



        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/ButtonSliderManage"
            android:layout_width="151dp"
            android:layout_height="34dp"
            android:layout_marginTop="2.5dp"
            android:layout_marginLeft="153dp"
            android:background="@drawable/buttonslider"
            app:layout_constraintLeft_toLeftOf="@+id/Slider"
            app:layout_constraintTop_toTopOf="@+id/Slider"
            tools:ignore="MissingConstraints"
            android:text="Housekeeping"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:fontFamily="@font/librefranklinmedium"
            />



    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.drawerlayout.widget.DrawerLayout>