<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="146dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/frame_979"
    android:layout_marginLeft="12dp"
    android:layout_marginRight="12dp"
    android:layout_marginTop="10dp">

    <ImageView
        android:id="@+id/imagePersonel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ava_personel_over"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="14dp"
        android:layout_marginLeft="25dp" />

    <TextView
        android:id="@+id/tvNamePersonel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="14dp"
        android:layout_marginLeft="142dp"
        android:text="Trần Thanh Hiền"
        android:textSize="23dp"
        android:fontFamily="@font/librefranklinbold"
        android:textColor="#732D2B"
        />
    <TextView
        android:id="@+id/tvPositionPersonel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="44dp"
        android:layout_marginLeft="142dp"
        android:text="Coolie"
        android:textSize="19dp"
        android:fontFamily="@font/librefranklinregular"
        android:textColor="#732D2B"
        />
    <TextView
        android:id="@+id/tvIDPersonel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="67dp"
        android:layout_marginLeft="142dp"
        android:text="ID:"
        android:textSize="12dp"
        android:fontFamily="@font/librefranklinregular"
        android:textColor="#732D2B"
        />
    <TextView
        android:id="@+id/tvIDofPersonel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="67dp"
        android:layout_marginLeft="163dp"
        android:text="2607"
        android:textSize="12dp"
        android:fontFamily="@font/librefranklinregular"
        android:textColor="#732D2B"
        />
    <TextView
        android:id="@+id/tvEmailPersonel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="90dp"
        android:layout_marginLeft="142dp"
        android:text="Email"
        android:textSize="12dp"
        android:fontFamily="@font/librefranklinregular"
        android:textColor="#732D2B"
        />
    <TextView
        android:id="@+id/tvEmailofPersonel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="90dp"
        android:layout_marginLeft="182dp"
        android:text="hientt@uit.edu.vn"
        android:textSize="12dp"
        android:fontFamily="@font/librefranklinregular"
        android:textColor="#732D2B"
        />
    <TextView
        android:id="@+id/tvPhonePersonel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="112dp"
        android:layout_marginLeft="142dp"
        android:text="Phone No."
        android:textSize="12dp"
        android:fontFamily="@font/librefranklinregular"
        android:textColor="#732D2B"
        />
    <TextView
        android:id="@+id/tvPhoneofPersonel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="112dp"
        android:layout_marginLeft="203dp"
        android:text="0456879567 "
        android:textSize="12dp"
        android:fontFamily="@font/librefranklinregular"
        android:textColor="#732D2B"
        />

</androidx.constraintlayout.widget.ConstraintLayout>
