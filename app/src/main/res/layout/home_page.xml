<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFF0CB"
    tools:context=".MainActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="287dp"
        android:background="@drawable/bg"

        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="73dp"
        android:layout_height="41dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/logo"
        android:layout_marginTop="54dp"
        android:layout_marginLeft="267dp"
        tools:ignore="MissingConstraints" />

    <RelativeLayout
        android:id="@+id/plastic"
        android:layout_width="341dp"
        android:layout_height="236dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:background="@drawable/plastic_bg"

        android:layout_marginTop="108dp"
        tools:ignore="MissingConstraints"
        />

    <TextView
        android:id="@+id/tv1"
        android:layout_width="101dp"
        android:layout_height="30dp"
        app:layout_constraintLeft_toLeftOf="@+id/plastic"
        app:layout_constraintTop_toTopOf="@+id/plastic"
        app:layout_constraintRight_toRightOf="@+id/plastic"
        android:layout_marginTop="26dp"
        android:layout_marginRight="201dp"
        android:text="Today"
        android:textSize="35dp"
        android:lineSpacingExtra="-12sp"
        android:gravity="top"
        android:fontFamily="@font/canelamediumtrial"
        android:textColor="#430000"
        />

    <TextView
        android:id="@+id/tv2"
        android:layout_width="88dp"
        android:layout_height="30dp"
        app:layout_constraintLeft_toLeftOf="@+id/plastic"
        app:layout_constraintTop_toTopOf="@+id/plastic"
        app:layout_constraintRight_toRightOf="@+id/plastic"
        android:layout_marginTop="69dp"
        android:layout_marginRight="212dp"
        android:text="11:03 AM"
        android:textSize="20dp"
        android:fontFamily="@font/canelamediumtrial"
        android:textColor="#430000"
        tools:ignore="DuplicateIds" />

    <TextView
        android:id="@+id/tv3"
        android:layout_width="129dp"
        android:layout_height="30dp"
        app:layout_constraintLeft_toLeftOf="@+id/tv2"
        app:layout_constraintTop_toTopOf="@+id/plastic"
        app:layout_constraintRight_toRightOf="@+id/plastic"
        android:layout_marginTop="69dp"
        android:layout_marginLeft="43dp"
        android:text="26th October"
        android:textSize="20dp"
        android:fontFamily="@font/canelamediumtrial"
        android:textColor="#430000"
        tools:ignore="DuplicateIds" />



    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/boxSales"
    android:layout_width="165dp"
    android:layout_height="71dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="437dp"
        android:layout_marginRight="183dp"
        android:background="@drawable/frame_979"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/tvSales"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@+id/boxSales"
        app:layout_constraintTop_toTopOf="@+id/boxSales"
        app:layout_constraintRight_toRightOf="@+id/boxSales"
        android:layout_marginTop="28dp"
        android:layout_marginLeft="83dp"
        android:text="Sales"
        android:textSize="15dp"
        android:fontFamily="@font/librefranklinmedium"
        android:textColor="#430000"
        />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/boxBooking"
        android:layout_width="165dp"
        android:layout_height="71dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="437dp"
        android:layout_marginLeft="183dp"
        android:background="@drawable/frame_979"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/tvBooking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@+id/boxBooking"
        app:layout_constraintTop_toTopOf="@+id/boxBooking"
        app:layout_constraintRight_toRightOf="@+id/boxBooking"
        android:layout_marginTop="28dp"
        android:layout_marginLeft="83dp"
        android:text="Booking"
        android:textSize="15dp"
        android:fontFamily="@font/librefranklinmedium"
        android:textColor="#430000"
        />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/boxGuestList"
        android:layout_width="165dp"
        android:layout_height="71dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="513dp"
        android:layout_marginLeft="183dp"
        android:background="@drawable/frame_979"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/tvGuestList"
        android:layout_width="wrap_content"
        android:layout_height="15dp"
        app:layout_constraintLeft_toLeftOf="@+id/boxGuestList"
        app:layout_constraintTop_toTopOf="@+id/boxGuestList"
        app:layout_constraintRight_toRightOf="@+id/boxGuestList"
        android:layout_marginTop="28dp"
        android:layout_marginLeft="83dp"
        android:text="Guest List"
        android:textSize="15dp"
        android:fontFamily="@font/librefranklinmedium"
        android:textColor="#430000"
        />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/boxRooms"
        android:layout_width="165dp"
        android:layout_height="71dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="513dp"
        android:layout_marginRight="183dp"
        android:background="@drawable/frame_979"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/tvRooms"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@+id/boxRooms"
        app:layout_constraintTop_toTopOf="@+id/boxRooms"
        app:layout_constraintRight_toRightOf="@+id/boxRooms"
        android:layout_marginTop="28dp"
        android:layout_marginLeft="83dp"
        android:text="Rooms"
        android:textSize="15dp"
        android:fontFamily="@font/librefranklinmedium"
        android:textColor="#430000"
        />



</androidx.constraintlayout.widget.ConstraintLayout>